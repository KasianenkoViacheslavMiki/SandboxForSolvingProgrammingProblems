<Application x:Class="SandboxForSolvingProgrammingProblems.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SandboxForSolvingProgrammingProblems">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="Theme">
                    <ResourceDictionary.MergedDictionaries>
                        <ResourceDictionary Source="Resources/Theme/Default.xaml"/>
                    </ResourceDictionary.MergedDictionaries>
                </ResourceDictionary>
                <ResourceDictionary x:Name="CustomUI">
                    <ResourceDictionary.MergedDictionaries>
                        <ResourceDictionary Source="Resources/CustomUI/Style.xaml"/>
                        <ResourceDictionary Source="Resources/CustomUI/ComboBox.xaml"/>
                    </ResourceDictionary.MergedDictionaries>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
